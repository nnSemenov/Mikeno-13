FoamFile {
    format ascii;
    class dictonary;
    location "system";
    object "fvSolution";
    version 2.0;
}

solvers {
    p {
        solver GAMG;
        smoother DIC;
        tolerance 1e-4;
        relTol 1e-2;
    }
    U {
        solver GAMG;
        smoother DILUGaussSeidel;
        tolerance 1e-4;
        relTol 1e-2;
    }
    "(e|h)" {
        solver GAMG;
        smoother DILUGaussSeidel;
        tolerance 1e-4;
        relTol 0.01;
    }
}

SIMPLE {
    consistent yes;
    momentumPredictor yes;
    nOuterCorrectors 1;
    nCorrectors     1;
    nNonOrthogonalCorrectors 0;

    residualControl {
        p               1e-3;
        U               1e-3;
        "(h|e)" 1e-3;
    }
}

relaxationFactors {
    equations {
        U 0.95;
        "(e|h)" 0.9;
    }
    fields {
        p 0.95;
    }
}
