FoamFile {
    class dictionary;
    format ascii;
    object fvModels;
}

porosity {
    type            porosityForce;

    porosityForceCoeffs
    {
        cellZone        inert;

        type            DarcyForchheimer;

        // D 100;  // Very little blockage
        // D 200;  // Some blockage but steady flow
        // D 500;  // Slight waviness in the far wake
        D 1e8; // Fully shedding behavior

        d   ($D $D $D);
        f   (0 0 0);

        coordinateSystem porousBlockage;
    }
}