set(ZOLTAN_TYPE @ZOLTAN_TYPE@)
set(ParaView_TYPE @ParaView_TYPE@)

# message(STATUS "PACKAGE_PREFIX_DIR = ${PACKAGE_PREFIX_DIR}")
cmake_path(GET PACKAGE_PREFIX_DIR PARENT_PATH FOAM_LIBBIN)
cmake_path(GET FOAM_LIBBIN PARENT_PATH FOAM_LIBBIN)
cmake_path(GET FOAM_LIBBIN PARENT_PATH install_prefix)
set(FOAM_APPBIN ${install_prefix}/bin)

cmake_path(GET install_prefix FILENAME options_in_path)
if(NOT ${options_in_path} STREQUAL ${WM_OPTIONS})
    message(FATAL_ERROR "Libraries and executables are installed to incorrect destination. WM_OPTIONS is ${WM_OPTIONS}, but \"${options_in_path}\" is deduced from installation")
endif()

cmake_path(GET install_prefix PARENT_PATH platforms_dir)
cmake_path(GET platforms_dir  PARENT_PATH WM_PROJECT_DIR)
cmake_path(GET WM_PROJECT_DIR PARENT_PATH WM_PROJECT_INST_DIR)
set(FOAM_INST_DIR ${WM_PROJECT_INST_DIR})
set(WM_DIR ${WM_PROJECT_DIR}/wmake)
set(FOAM_ETC ${WM_PROJECT_DIR}/etc)
set(FOAM_SRC ${WM_PROJECT_DIR}/src)
set(FOAM_APP ${WM_PROJECT_DIR}/applications)
set(FOAM_TUTORIALS ${WM_PROJECT_DIR}/tutorials)
set(FOAM_UTILITIES ${FOAM_APP}/utilities)
set(FOAM_SOLVERS ${FOAM_APP}/solvers)
set(FOAM_MODULES ${FOAM_APP}/modules)

# Include targets
include(${CMAKE_CURRENT_LIST_DIR}/MikenoTargets.cmake)