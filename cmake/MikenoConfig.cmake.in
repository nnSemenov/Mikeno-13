set(Mikeno_VERSION @PROJECT_VERSION@)

@PACKAGE_INIT@

set(FOAM_regular_executables @FOAM_regular_executables@)
set(FOAM_regular_libraries @FOAM_regular_libraries@)
set(FOAM_special_libraries @FOAM_special_libraries@)

set(WM_COMPILER @WM_COMPILER@)
set(WM_PRECISION_OPTION @WM_PRECISION_OPTION@)
set(WM_MPLIB @WM_MPLIB@)
set(WM_OPTIONS @WM_OPTIONS@)
set(WM_ARCH @WM_ARCH@)
set(WM_THIRD_PARTY @WM_THIRD_PARTY@)
set(WM_LABEL_SIZE @WM_LABEL_SIZE@)
set(WM_PROJECT @WM_PROJECT@)
set(WM_LABEL_OPTION @WM_LABEL_OPTION@)
set(WM_CC @WM_CC@)
set(WM_CFLAGS "@WM_CFLAGS@")
set(WM_LINK_LANGUAGE "@WM_LINK_LANGUAGE@")
set(WM_OSTYPE @WM_OSTYPE@)
set(WM_PROJECT_VERSION @WM_PROJECT_VERSION@)
set(WM_ARCH_OPTION @WM_ARCH_OPTION@)
set(WM_CXXFLAGS "@WM_CXXFLAGS@")
set(WM_LDFLAGS "@WM_LDFLAGS@")
set(WM_CXX @WM_CXX@)
set(WM_COMPILE_OPTION @WM_COMPILE_OPTION@)
set(WM_COMPILER_LIB_ARCH @WM_COMPILER_LIB_ARCH@)

set(FOAM_MPI @FOAM_MPI@)
